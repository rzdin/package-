pipeline {
  agent any
  tools {nodejs "NODEJS"}    
  stages {
    stage('build') {
      steps {
          sh 'npm install'
          sh 'zip myfile-$BUILD_NUMBER-$TIMESTAMP.zip db  handler  jenkinsfile  LICENSE  model  node_modules  package.json  README.md  serverless.yml  utils'
      }
    }
  }
}
